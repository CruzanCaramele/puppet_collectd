LoadPlugin "apache"
<Plugin "apache">
    <%- @instances.each do |instance_instance, instance_config| -%>
    <Instance "<%= instance_instance %>">
        <%- instance_config.each do |key,value| -%>
        <%= key -%> <%= value %>
        <%- end -%>
    </Instance>
    <%- end -%>
</Plugin>

